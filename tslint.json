{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true, "single", "jsx-double"
    ],
    "interface-name": [
      true, "never-prefix"
    ],
    "semicolon": [
      true, "never"
    ],
    "no-consecutive-blank-lines": [
      true, 2
    ]
  },
  "rulesDirectory": []
}
